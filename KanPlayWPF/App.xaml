<Application x:Class="KanPlayWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views\MainWindow.xaml"
             Startup="Application_Startup">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="pack://application:,,,/Styles/ColorAndBrushes.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/ControlStyles.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/TableDefines.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>

            
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
